{
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "moduleResolution": "bundler",
    "module": "es2020",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "traceResolution": false,
    "baseUrl": ".",
    "paths": {
      "@hicommonwealth/*": ["libs/*/src"]
    }
  },
  "files": [],
  "references": [
    { "path": "libs/adapters" },
    { "path": "libs/chains" },
    { "path": "libs/core" },
    { "path": "libs/logging" },
    { "path": "libs/model" },
    { "path": "libs/shared" },
    { "path": "packages/commonwealth" },
    { "path": "packages/discord-bot" },
    { "path": "packages/scripts" },
    { "path": "packages/snapshot-listener" }
  ]
}
