{
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "traceResolution": false,
    "baseUrl": ".",
    "paths": {
      "common-common/*": ["packages/common-common/*"]
    }
  },
  "include": ["libs/core/src"]
}
