{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "outDir": "./build",
    "skipLibCheck": true,
    "paths": {
      "common-common/*": ["packages/common-common/*"],
      "token-balance-cache/*": ["packages/token-balance-cache/*"],
      "*": [
        "packages/commonwealth/*",
        "packages/commonwealth/shared/*",
        "packages/commonwealth/client/scripts/*"
      ]
    }
  },
  "include": [
    "server.ts",
    "./client",
    "./client/scripts",
    "./shared",
    "./globals.d.ts",
    "./server",
    "./scripts"
  ]
}
